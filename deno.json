{
  "imports": {
    "@/": "./src/",
    "@models/": "./src/models/",
    "@routes/": "./src/routes/",
    "@utils/": "./src/utils/"
  },
  "tasks": {
    "safe": "deno lint && deno check --all ./src/**/*.ts",
    "start": "deno run --allow-net --allow-write --allow-read ./src/main.ts"
  },
  "compilerOptions": {
    "strict": true
  },
  "lint": {
    "include": [
      "src/"
    ]
  }
}